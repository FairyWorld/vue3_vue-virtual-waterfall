{
  "tabWidth": 4,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "printWidth": 320,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "vueIndentScriptAndStyle": false,
  "singleAttributePerLine": false,
  "plugins": [
    "prettier-plugin-rational-order"
  ]
}